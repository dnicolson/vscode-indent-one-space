{
	"compilerOptions": {
		"module": "commonjs",
		"target": "ES2019",
		"outDir": "out",
		"lib": [
			"ES2019"
		],
		"sourceMap": true,
		"rootDir": "src",
		"strict": true,
		"esModuleInterop": true,
		"forceConsistentCasingInFileNames": true,
		"skipLibCheck": true
	},
	"exclude": [
		"node_modules"
	]
}